<?xml version="1.0"?>
<object name="ExtropyFeature" meta_type="Factory-based Type Information"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">Deliverable</property>
 <property
    name="description">A Deliverable is a clearly defined unit that should be produced as part of a project. Deliverables contain tasks.</property>
 <property name="content_icon">feature_icon.gif</property>
 <property name="content_meta_type">ExtropyFeature</property>
 <property name="product">Extropy</property>
 <property name="factory">addExtropyFeature</property>
 <property name="immediate_view">base_edit</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="ExtropyTask"/>
  <element value="Document"/>
  <element value="Event"/>
  <element value="File"/>
  <element value="Image"/>
  <element value="Memo"/>
 </property>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="extropyfeature_view"/>
 <alias from="view" to="(Default)"/>
 <alias from="edit" to="base_edit"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/extropyfeature_view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/base_edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="Timereport" action_id="timereport" category="object"
    condition_expr="" url_expr="string:${object_url}/timereport"
    visible="True">
  <permission value="View"/>
 </action>
</object>
