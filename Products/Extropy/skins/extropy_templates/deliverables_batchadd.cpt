<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      lang="en"
      i18n:domain="extropy"
      metal:use-macro="here/main_template/macros/master">
  <head>
  </head>
<body>
<div metal:fill-slot="main">
    
    <form name="batchadd_form"
          action="."
          method="post"
          enctype="multipart/form-data"
          tal:attributes="action string:${here/getId}/${template/getId}" >

        <input type="hidden" name="form.submitted" value="1" />

        <h2>Add Deliverables for this phase</h2>
      
        <div tal:repeat="item python:range(0,10)"
             style="margin-top : 1em" tal:define="icon here/feature_icon.gif">
            <img tal:replace="structure icon" /><input type="text" size="50" style="width : 40em" name="featureadd:list" /> 
        </div>

        <div style="margin : 2em 1em;">
            <input class="context" 
                   name="form.button.Update" 
                   type="submit" 
                   value="Add all" />
        </div>
    </form>
</div>
</body>
</html>
