<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Participant"/>
    <role name="Customer"/>
    <role name="Finance-manager"/>
  </roles>
  <permissions>
      <permission name="Extropy: Finances" acquire="True">
                <role name="Finance-manager"/>
      </permission>

      <permission name="Extropy: Participate" acquire="True">
                <role name="Manager"/>
                <role name="Participant"/>
      </permission>

      <permission name="Extropy: Manage Projects" acquire="True">
                <role name="Manager"/>  
      </permission>

      <permission name="Extropy: Observe" acquire="True">
                <role name="Customer"/>
                <role name="Participant"/>
                <role name="Manager"/>

      </permission>
      
  </permissions>
</rolemap>
