var livesearch=(function(){var g,f,e,c;g=400;f=400;e={};c="LSHighlight";function d(t,i){var j,q,s,u,h,k,p;j=null;q=null;s={};u="livesearch_reply";u=t.attr("action").replace(/search$/g,"")+u;h=t.find("div.LSResult");k=t.find("div.LSShadow");p=t.find("input[name=path]");function l(){jQuery(h).find(".livesearchContainer, .LSIEFix").slideUp("fast",function(){jQuery(h).find(".toolTipArrow").fadeOut(100,function(){h.hide();jQuery(h).find(".toolTipArrow").show()})});j=null}function n(){window.setTimeout('livesearch.hide("'+t.attr("id")+'")',f)}function o(v){if((jQuery(h).find(".livesearchContainer").length===0)||jQuery(h).find(".livesearchContainer").is(":hidden")){k.html(v);jQuery(h).find(".livesearchContainer").hide();h.show();jQuery(".livesearchContainer").animate({opacity:"toggle",height:"toggle"},"fast","swing")}else{h.show();k.html(v)}}function r(){if(j===i.value){return}j=i.value;if(q&&q.readyState<4){q.abort()}if(i.value.length<2){l();return}var v={q:i.value};if(p.length&&p[0].checked){v.path=p.val()}v=jQuery.param(v);if(s[v]){o(s[v]);return}q=jQuery.get(u,v,function(w){o(w);s[v]=w},"text")}function m(){window.setTimeout('livesearch.search("'+t.attr("id")+'")',g)}return{hide:l,hide_delayed:n,search:r,search_delayed:m}}function b(s){var j,h,m,n,i,l;j=null;h=s.find("div.LSResult");m=s.find("div.LSShadow");function k(){n=m.find("li.LSHighlight").removeClass(c);i=n.prev("li");if(!i.length){i=m.find("li:last")}i.addClass(c);return false}function p(){n=m.find("li.LSHighlight").removeClass(c);l=n.next("li");if(!l.length){l=m.find("li:first")}l.addClass(c);return false}function q(){m.find("li.LSHighlight").removeClass(c);h.hide()}function r(t){window.clearTimeout(j);switch(t.keyCode){case 38:return k();case 40:return p();case 27:return q();case 37:break;case 39:break;default:j=window.setTimeout('livesearch.search("'+s.attr("id")+'")',g)}}function o(){var t=m.find("li.LSHighlight a").attr("href");if(!t){return}window.location=t;return false}return{handler:r,submit:o}}function a(j){var l,h,k;l="livesearch"+j;h=jQuery(this).parents("form:first");k=b(h);e[l]=d(h,this);h.attr("id",l).css("white-space","nowrap").submit(k.submit);jQuery(this).attr("autocomplete","off").keydown(k.handler).focus(e[l].search_delayed).blur(e[l].hide_delayed)}jQuery(function(){jQuery("#searchGadget,input.portlet-search-gadget").each(a)});return{search:function(h){e[h].search()},hide:function(h){e[h].hide()}}}());