function wrapNode(a,c,b){jQuery(a).wrap("<"+c+">").parent().addClass(b)}function nodeContained(b,a){return jQuery(b).parents().filter(function(){return this===a}).length>0}function findContainer(b,a){var c=jQuery(b).parents().filter(a);return c.length?c.get(0):false}function hasClassName(a,b){return jQuery(a).hasClass(b)}function addClassName(a,b){jQuery(a).addClass(b)}function removeClassName(a,b){jQuery(a).removeClass(b)}function replaceClassName(c,a,b,d){if(d||jQuery(c).hasClass(a)){jQuery(c).removeClass(a).addClass(b)}}function walkTextNodes(b,a,c){jQuery(b).find("*").andSelf().contents().each(function(){if(this.nodeType===3){a(this,c)}})}function getInnerTextCompatible(a){return jQuery(a).text()}function getInnerTextFast(a){return jQuery(a).text()}function sortNodes(c,e,d){var b,a;b=function(f){this.value=e(f);this.cloned_node=f.cloneNode(true)};b.prototype.toString=function(){return this.value.toString?this.value.toString():this.value};a=jQuery(c).map(function(){return new b(this)});if(d){a.sort(d)}else{a.sort()}jQuery.each(a,function(f){jQuery(c[f]).replace(this.cloned_node)})}function copyChildNodes(a,b){jQuery(a).children().clone().appendTo(jQuery(b))};