if(typeof(plone)==="undefined"){var plone={}}(function(a){plone.UnlockHandler={init:function(){if(a("form.enableUnlockProtection").length){a(window).unload(plone.UnlockHandler.execute);plone.UnlockHandler._refresher=setInterval(plone.UnlockHandler.refresh,300000)}},cleanup:function(){a(window).unbind("unload",plone.UnlockHandler.execute);clearInterval(plone.UnlockHandler._refresher)},execute:function(){if(this.submitting){return}a.get(plone.UnlockHandler._baseUrl()+"/@@plone_lock_operations/safe_unlock")},refresh:function(){if(this.submitting){return}a.get(plone.UnlockHandler._baseUrl()+"/@@plone_lock_operations/refresh_lock")},_baseUrl:function(){var c,b;c=a("base").attr("href");if(!c){b=window.location.href.split("/");b.pop();c=b.join("/")}return c}};a(plone.UnlockHandler.init)})(jQuery);