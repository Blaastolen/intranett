(function(d){function a(f){var g=new RegExp("sortabledata-([^ ]*)","g");var h=g.exec(f.attr("class"));if(h){return h[1]}else{return null}}function e(f){var g=a(f);if(g==null){g=f.text()}if(g.charAt(4)!="-"&&g.charAt(7)!="-"&&!isNaN(parseFloat(g))){return parseFloat(g)}return g.toLowerCase()}function c(){var l=d(this).closest("th");var k=d("th",d(this).closest("thead")).index(l);var j=d(this).parents("table:first");var i=j.find("tbody:first");var h=j.attr("sorted")==k;d(this).parent().find("th:not(.nosort) .sortdirection").html("&#x2003;");d(this).children(".sortdirection").html(h?"&#x25b2;":"&#x25bc;");var g=d(this).parent().children("th").index(this);var m=[];var f=true;i.find("tr").each(function(){var n=d(this).children("td");var o=e(n.slice(g,g+1));if(isNaN(o)){f=false}m.push([o,e(n.slice(1,2)),e(n.slice(0,1)),this])});if(m.length){if(f){m.sort(function(o,n){return o[0]-n[0]})}else{m.sort()}if(h){m.reverse()}j.attr("sorted",h?"":k);i.append(d.map(m,function(n){return n[3]}));i.each(b)}}function b(){var f=d(this);f.find("tr").removeClass("odd").removeClass("even").filter(":odd").addClass("even").end().filter(":even").addClass("odd")}d(function(){var f=d("<span>&#x2003;</span>").addClass("sortdirection");d("table.listing:not(.nosort) thead th:not(.nosort)").append(f.clone()).css("cursor","pointer").click(c);d("table.listing:not(.nosort) tbody").each(b)})})(jQuery);