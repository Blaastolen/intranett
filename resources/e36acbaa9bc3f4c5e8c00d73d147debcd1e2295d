function createCookie(c,d,e){var b,a;if(e){b=new Date();b.setTime(b.getTime()+(e*24*60*60*1000));a="; expires="+b.toGMTString()}else{a=""}document.cookie=c+"="+escape(d)+a+"; path=/;"}function readCookie(b){var e=b+"=",a=document.cookie.split(";"),d,f;for(d=0;d<a.length;d=d+1){f=a[d];while(f.charAt(0)===" "){f=f.substring(1,f.length)}if(f.indexOf(e)===0){return unescape(f.substring(e.length,f.length))}}return null};