function activateCollapsibles(){(function(a){a("dl.collapsible:not([class$=Collapsible])").find("dt.collapsibleHeader:first").click(function(){var c=a(this).parents("dl.collapsible:first"),b;if(!c){return true}b=c.hasClass("inline")?"Inline":"Block";c.toggleClass("collapsed"+b+"Collapsible").toggleClass("expanded"+b+"Collapsible")}).end().each(function(){var c=a(this).hasClass("collapsedOnLoad")?"collapsed":"expanded",b=a(this).hasClass("inline")?"Inline":"Block";a(this).removeClass("collapsedOnLoad").addClass(c+b+"Collapsible")})}(jQuery))}jQuery(function(a){a(activateCollapsibles)});