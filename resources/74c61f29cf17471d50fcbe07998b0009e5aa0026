(function(a){a.fn.do_search_collapse=function(){function b(c){var d=a(c);if(d.find("input[id$=_toggle]:checkbox").length>0){if(d.find("input[id$=_toggle]:checkbox:checked").length===0){return true}}if(d.find(":text[value]").length>0){return true}if(d.find("select .default_option").length>0){if(d.find("select .default_option:selected").length===0){return true}}return false}return this.each(function(){var c=a(this).find(".collapser:first"),d=a(this).find(".collapse:first");c.click(function(){var e=a(this).parent(),f=e.find(".collapse:first");f.slideToggle("normal");a(this).toggleClass("expanded");a(this).toggleClass("collapsed")});if(b(this)){c.addClass("expanded")}else{d.hide();c.addClass("collapsed")}})};jQuery(function(b){b(".field.collapsible").do_search_collapse()})}(jQuery));