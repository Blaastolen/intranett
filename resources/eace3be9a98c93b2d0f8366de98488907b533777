var ploneInputLabel={focus:function(){var a=jQuery(this);if(a.hasClass("inputLabelActive")&&a.val()===a.attr("title")){a.val("").removeClass("inputLabelActive")}if(a.hasClass("inputLabelPassword")){ploneInputLabel._setInputType(a.removeClass("inputLabelPassword"),"password").focus().bind("blur.ploneInputLabel",ploneInputLabel.blur)}},blur:function(b){var a=jQuery(this);if(a.is(':password[value=""]')){a=ploneInputLabel._setInputType(this,"text").addClass("inputLabelPassword").bind("focus.ploneInputLabel",ploneInputLabel.focus);if(b.originalEvent&&b.originalEvent.explicitOriginalTarget){jQuery(b.originalEvent.explicitOriginalTarget).trigger("focus!")}}if(!a.val()){a.addClass("inputLabelActive").val(a.attr("title"))}},submit:function(){jQuery("input[title].inputLabelActive").trigger("focus.ploneInputLabel")},_setInputType:function(d,b){var e=jQuery,a,c;a=new RegExp('type="?'+e(d).attr("type")+'"?');c=e(e("<div></div>").append(e(d).clone()).html().replace(a,"").replace(/\/?>/,'type="'+b+'" />'));e(d).replaceWith(c);return c}};jQuery(function(a){a("form:has(input[title].inputLabel)").submit(ploneInputLabel.submit);a("input[title].inputLabel").bind("focus.ploneInputLabel",ploneInputLabel.focus).bind("blur.ploneInputLabel",ploneInputLabel.blur).trigger("blur.ploneInputLabel")});